<?xml version="1.0" encoding="utf-8"?><android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/parentCardview"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="199dp"    android:layout_margin="10dp"    android:layout_marginBottom="6dp"    app:cardElevation="4dp"    app:cardCornerRadius="2dp"    app:cardUseCompatPadding="true"    app:layout_constraintBottom_toBottomOf="parent"    app:layout_constraintTop_toTopOf="parent">    <android.support.constraint.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <ImageView            android:scaleType="fitXY"            android:id="@+id/estabImage"            android:layout_width="383dp"            android:layout_height="150dp"            android:layout_alignParentStart="true"            android:layout_alignParentTop="true"            android:layout_marginStart="0dp"            android:layout_marginTop="0dp"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <View            android:id="@+id/view"            android:layout_width="383dp"            android:layout_height="150dp"            android:layout_marginStart="0dp"            android:layout_marginTop="0dp"            android:background="@drawable/bgshadow"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <com.github.siyamed.shapeimageview.mask.PorterCircularImageView            android:id="@+id/roundedImageViewEstab"            android:layout_width="81dp"            android:layout_height="71dp"            android:layout_marginStart="16dp"            android:layout_marginBottom="16dp"            android:scaleType="fitXY"            app:layout_constraintBottom_toTopOf="@+id/estabLocation"            app:layout_constraintStart_toStartOf="parent"            app:siBorderColor="@color/white"            app:siRadius="2dp"            app:siBorderWidth="2dp"/>        <TextView            android:id="@+id/nbrReviewAllResto"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginTop="8dp"            android:layout_marginEnd="8dp"            android:drawableStart="@drawable/ic_reviews"            android:text="252"            android:textColor="@color/white"            android:textSize="14dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/estabName"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentTop="true"            android:layout_marginStart="8dp"            android:layout_marginBottom="36dp"            android:layout_toEndOf="@id/estabImage"            android:text="Le Bargoum"            android:textColor="@color/white"            android:textSize="18dp"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="@+id/view"            app:layout_constraintStart_toEndOf="@+id/roundedImageViewEstab" />        <TextView            android:id="@+id/estabSubCat"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_above="@+id/estabRating"            android:layout_marginStart="8dp"            android:layout_marginTop="8dp"            android:layout_marginBottom="8dp"            android:layout_toEndOf="@id/estabImage"            android:text="Disco"            android:textColor="@color/white"            android:textSize="12dp"            app:layout_constraintBottom_toBottomOf="@+id/view"            app:layout_constraintStart_toEndOf="@+id/roundedImageViewEstab"            app:layout_constraintTop_toBottomOf="@+id/estabName"            app:layout_constraintVertical_bias="0.0" />        <TextView            android:id="@+id/estabRating"            android:layout_width="50dp"            android:layout_height="30dp"            android:layout_below="@+id/estabImage"            android:layout_marginEnd="8dp"            android:layout_marginBottom="16dp"            android:background="@color/seek_green"            android:text="3.4"            android:textAlignment="center"            android:textColor="@color/white"            android:textSize="20dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent" />        <TextView            android:id="@+id/estabLocation"            android:layout_width="231dp"            android:layout_height="21dp"            android:layout_below="@id/estabRating"            android:layout_marginStart="16dp"            android:layout_marginBottom="4dp"            android:drawableStart="@drawable/ic_location"            android:text="Menzah 5"            android:textSize="14dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/view"            app:layout_constraintVertical_bias="1.0" />    </android.support.constraint.ConstraintLayout></android.support.v7.widget.CardView>